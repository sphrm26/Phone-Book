@using PhoneBook
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html>
<head>
    <title>Creative Colorlib SignUp Form</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link rel="stylesheet" href="~/css/Home.css" />
    <script src="~/js/bootstrap-rtl.js"></script>
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <link href="//fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,700,700i" rel="stylesheet">
</head>
<body>


    <div class="main-w3layouts wrapper">
        <h1>Phone Book</h1>





        <div id="Login" class="main-agileinfo">
            <div class="agileits-top">
                <form onsubmit="GoToPanel()">
                    <input class="text" type="text" Id="OTP" placeholder="one time password" required="">
                    <div class="wthree-text">
                        <div class="clear"> </div>
                    </div>
                    <p>please check your spam</p>
                    <input type="submit" value="Enter">
                </form>
            </div>
        </div>




        <ul class="colorlib-bubbles">
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
        </ul>
    </div>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>

        function GoToPanel() {
            event.preventDefault();

            window.location.href = "/UserPanel/Panel/";
        }
    </script>
</body>
</html>